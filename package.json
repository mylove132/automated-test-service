{
  "name": "automated-test-service",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start:prod": "export NODE_ENV=production && node dist/main.js",
    "start:test": "export NODE_ENV=test && node dist/main.js",
    "start:test:watch": "export NODE_ENV=test && nest start --watch",
    "start": "export NODE_ENV=development && node dist/main.js",
    "start:watch": "export NODE_ENV=development  && nest start --watch",
    "prestart:prod": "tsc",
    "test": "jest --config=jest.json",
    "test:watch": "jest --watch --config=jest.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -n migration",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "typeorm:migration:revert": "npm run typeorm -- migration:revert",
    "pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/write-type-orm-config.ts",
    "migration": "export NODE_ENV=test && npm run typeorm:migration:generate && npm run typeorm:migration:run",
    "migration:dev": "export NODE_ENV=development && npm run typeorm:migration:generate && npm run typeorm:migration:run"
  },
  "dependencies": {
    "@nestjs/bull": "0.0.1-beta.4",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/platform-socket.io": "^6.10.13",
    "@nestjs/schedule": "^0.3.1",
    "@nestjs/swagger": "^3.0.1",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjs/websockets": "^6.10.13",
    "bull": "^3.13.0",
    "child_process": "^1.0.2",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.14.0",
    "cron": "^1.8.2",
    "cron-parser": "^2.13.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.1.2",
    "moment": "latest",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^1.0.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.0.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "socket.io-redis": "^5.2.0",
    "stacktrace-js": "^2.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-md5": "^1.2.7",
    "typeorm": "^0.2.15"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.4",
    "@nestjs/schematics": "^6.7.0",
    "@types/cron": "^1.7.2",
    "@types/ioredis": "^4.16.2",
    "@types/jest": "^24.0.18",
    "@types/mysql": "^2.15.10",
    "@types/node": "^12.7.5",
    "@types/redis": "^2.8.20",
    "@types/socket.io-redis": "^1.0.25",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
