{
  "name": "automated-test-service",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/anishkny/node-express-realworld-example-app.svg?branch=master)](https://travis-ci.org/anishkny/node-express-realworld-example-app)",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start:prod": "export NODE_ENV=production && node dist/main.js",
    "start:test": "export NODE_ENV=test && npm run typeorm:migration:run && node dist/main.js",
    "start": "export NODE_ENV=development && node index.js",
    "start:watch": "export NODE_ENV=development && nodemon",
    "prestart:prod": "tsc",
    "test": "jest --config=jest.json",
    "test:watch": "jest --watch --config=jest.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -n migration",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/write-type-orm-config.ts",
    "migration": "export NODE_ENV=test && npm run typeorm:migration:generate && npm run typeorm:migration:run",
    "migration:dev": "export NODE_ENV=development && npm run typeorm:migration:generate && npm run typeorm:migration:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lujakob/nestjs-realworld-example-app.git"
  },
  "author": "lu.jakob@googlemail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lujakob/nestjs-realworld-example-app/issues"
  },
  "homepage": "https://github.com/lujakob/nestjs-realworld-example-app#readme",
  "dependencies": {
    "@nestjs/common": "^6.0.1",
    "@nestjs/core": "^6.0.1",
    "@nestjs/microservices": "^6.0.1",
    "@nestjs/platform-express": "^6.0.1",
    "@nestjs/schedule": "^0.1.1",
    "@nestjs/swagger": "^3.0.1",
    "@nestjs/testing": "^6.0.1",
    "@nestjs/typeorm": "^6.0.0",
    "@nestjs/websockets": "^6.0.1",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "cron": "^1.7.2",
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.9-1",
    "form-data": "^3.0.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.1.1",
    "log4js": "^6.1.0",
    "mysql": "^2.17.1",
    "nestjs-typeorm-paginate": "^1.0.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.15.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "rxjs": "^6.4.0",
    "slug": "^1.0.0",
    "swagger-ui-express": "^4.1.2",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.15",
    "typescript": "^3.3.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.21",
    "atob": ">=2.1.0",
    "deep-extend": ">=0.5.1",
    "extend": ">=3.0.2",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "supertest": "^3.0.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.5.4"
  }
}
